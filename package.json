{
  "name": "brewGet",
  "description": "brewGet is a web application that supports the non-monetary exchange of regionally-limited or otherwise difficult to acquire beer. Built with Node.js, Koa, and AngularJS.",
  "version": "0.1.1",
  "private": true,
  "author": {
    "name": "Michael J. Bondra",
    "email": "mjbondra@gmail.com",
    "url": "https://twitter.com/mjbondra"
  },
  "scripts": {
    "start": "NODE_ENV=development nodemon --harmony server",
    "test": "NODE_ENV=test PORT=3001 ./node_modules/.bin/mocha --harmony --reporter spec server/test/*.js",
    "install": "grunt"
  },
  "devDependencies": {
    "bower": "~1.2.8",
    "graceful-fs": "~2.0.1",
    "grunt": "~0.4.2",
    "grunt-bower-task": "~0.3.4",
    "grunt-cli": "~0.1.11",
    "grunt-contrib-uglify": "~0.2.7",
    "mocha": "~1.16.2",
    "should": "~2.1.1",
    "supertest": "~0.8.2"
  },
  "dependencies": {
    "busboy": "mscdex/busboy",
    "co-body": "~0.0.1",
    "co-busboy": "~0.1.0",
    "co-fs": "~1.1.1",
    "koa": "~0.2.1",
    "koa-compress": "~1.0.0",
    "koa-logger": "~1.1.0",
    "koa-router": "~2.2.0",
    "koa-session-mongoose": "~0.1.6",
    "koa-session-store": "~0.1.0",
    "koa-static-cache": "~1.0.0",
    "mongoose": "~3.8.3",
    "q": "~1.0.0",
    "qs": "~0.6.6",
    "sanitizer": "~0.1.1",
    "underscore": "~1.5.2",
    "uid2": "~0.0.3",
    "validator": "~2.0.0"
  }
}