{
  "name": "brewGet",
  "description": "brewGet is a web application that supports the non-monetary exchange of regionally-limited or otherwise difficult to acquire beer. Built with Node.js, Koa, and AngularJS.",
  "version": "0.1.1",
  "private": true,
  "author": {
    "name": "Michael J. Bondra",
    "email": "mjbondra@gmail.com",
    "url": "https://twitter.com/mjbondra"
  },
  "scripts": {
    "start": "NODE_ENV=development nodemon --harmony server",
    "test": "NODE_ENV=test PORT=3001 ./node_modules/.bin/mocha --harmony --reporter spec server/test/*.js",
    "install": "bower install && gulp"
  },
  "devDependencies": {
    "bower": "1.x",
    "gulp": "3.x",
    "gulp-compass": "1.x",
    "gulp-browserify": "0.x",
    "gulp-rename": "0.x",
    "gulp-uglify": "0.x",
    "mocha": "1.x",
    "should": "2.x",
    "supertest": "0.x"
  },
  "dependencies": {
    "busboy": "0.x",
    "co-body": "0.x",
    "co-busboy": "0.x",
    "co-fs": "1.x",
    "koa": "0.x",
    "koa-compress": "1.x",
    "koa-logger": "1.x",
    "koa-router": "2.x",
    "koa-session-mongoose": "0.x",
    "koa-session-store": "0.x",
    "koa-static-cache": "1.x",
    "mongoose": "3.8.3",
    "q": "1.x",
    "qs": "0.x",
    "sanitizer": "0.x",
    "underscore": "1.x",
    "uid2": "0.x",
    "validator": "2.x"
  }
}